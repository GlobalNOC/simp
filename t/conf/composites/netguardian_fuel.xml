<?xml version="1.0" encoding="UTF-8" ?>

<composite>
    <variables>
        <input name="node" value="node_name" />
        <scan poll_value="sensor_name" oid_suffix="sensor_id" oid="1.3.6.1.4.1.2682.1.2.6.1.3.*" />
    </variables>

    <data>
        <meta name="node" source="node_name" />
        <meta name="name" source="sensor_name" />
        <value name="fuel" source="1.3.6.1.4.1.2682.1.2.6.1.4.sensor_id" />
    </data>

    <conversions>
        <match pattern='(.*(Propane|Gas|Fuel).*)'>
            <data name="name" />
        </match>

        <match pattern='(\d+\.?\d*)'>
            <data name="fuel" />
        </match>
    </conversions>
</composite>
