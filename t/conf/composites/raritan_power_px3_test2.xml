<?xml version="1.0" encoding="UTF-8" ?>

<composite>
    <variables>
        <input name="node" value="node_name" required="1" />
        <scan poll_value="intf"     oid_suffix="outlet_id" oid="1.3.6.1.4.1.13742.6.3.5.3.1.3.1.*" />
	<scan poll_value="status"   oid_suffix="s_oid"     oid="1.3.6.1.4.1.13742.6.5.4.3.1.4.1.outlet_id.14"/>
	<scan poll_value="current"  oid_suffix="c_oid"     oid="1.3.6.1.4.1.13742.6.5.4.3.1.4.1.outlet_id.1"/>
	<scan poll_value="voltage"  oid_suffix="v_oid"     oid="1.3.6.1.4.1.13742.6.5.4.3.1.4.1.outlet_id.4"/>
	<scan poll_value="actpower" oid_suffix="act_oid"   oid="1.3.6.1.4.1.13742.6.5.4.3.1.4.1.outlet_id.5"/>
	<scan poll_value="apppower" oid_suffix="app_oid"   oid="1.3.6.1.4.1.13742.6.5.4.3.1.4.1.outlet_id.6"/>
	<scan poll_value="pf"       oid_suffix="pf_oid"    oid="1.3.6.1.4.1.13742.6.5.4.3.1.4.1.outlet_id.7"/>
    </variables>

    <data>
        <meta  name="node"     source="node_name" />
        <meta  name="intf"     source="outlet_name" />
        <value name="status"   source="status" />
        <value name="current"  source="current" />
        <value name="voltage"  source="voltage" />
        <value name="actpower" source="actpower" />
        <value name="apppower" source="apppower" />
        <value name="pf"       source="pf" />
    </data>

    <conversions>
        <function definition="${} 1000 /">
            <data name="current" />
        </function>
    </conversions>
</composite>
