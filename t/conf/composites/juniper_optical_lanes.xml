<?xml version="1.0" encoding="UTF-8" ?>

<composite>
    <variables>
        <scan poll_value="intf_idx" oid_suffix="intf_idx" oid="1.3.6.1.2.1.2.2.1.1.*" />
        <scan poll_value="lane_num" oid_suffix="lane_idx" oid="1.3.6.1.4.1.2636.3.60.1.2.1.1.1.intf_idx.**" />
    </variables>

    <data>
        <meta name="lane" source="lane_num" />
        <meta name="intf" source="1.3.6.1.2.1.31.1.1.1.1.intf_idx" />
        <value name="rxpower" source="1.3.6.1.4.1.2636.3.60.1.2.1.1.6.intf_idx.lane_idx" />
        <value name="txpower" source="1.3.6.1.4.1.2636.3.60.1.2.1.1.8.intf_idx.lane_idx" />
    </data>

    <conversions>
        <function definition='${} 100 /'>
            <data name="rxpower" />
            <data name="txpower" />
        </function>

        <replace this='${}' with='${}-lane${lane}'>
            <data name="intf" />
        </replace>
    </conversions>
</composite>
