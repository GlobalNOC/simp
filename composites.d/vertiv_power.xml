<?xml version="1.0" encoding="UTF-8" ?>

<composite>
    <variables>
        <scan value="intf_name" oid="1.3.6.1.4.1.21239.5.2.3.2.1.2.*">
            <match name="intf_id"/>
        </scan>
    </variables>

    <data>
        <meta name="intf" source="intf_name" />
        <value name="voltage" source="1.3.6.1.4.1.21239.5.2.3.2.1.4.intf_id" />
        <value name="current" source="1.3.6.1.4.1.21239.5.2.3.2.1.8.intf_id" />
        <value name="apppower" source="1.3.6.1.4.1.21239.5.2.3.2.1.12.intf_id" />
        <value name="actpower" source="1.3.6.1.4.1.21239.5.2.3.2.1.13.intf_id" />
        <value name="pf" source="1.3.6.1.4.1.21239.5.2.3.2.1.14.intf_id" />
    </data>

    <conversions>
        <function definition='${} 10 /'>
            <data name="voltage" />
        </function>

        <function definition='${} 100 /'>
            <data name="current" />
        </function>
    </conversions>
</composite>
