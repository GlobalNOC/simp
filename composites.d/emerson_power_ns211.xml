<?xml version="1.0" encoding="UTF-8" ?>

<composite>
    <variables>
        <scan value="intf_name" oid="1.3.6.1.4.1.6302.2.1.1.4.**">
            <match name="intf_id"/>
        </scan>
    </variables>

    <data>
        <meta name="intf" source="intf_name" />
        <value name="current" source="1.3.6.1.4.1.6302.2.1.2.3.intf_id" />
        <value name="voltage" source="1.3.6.1.4.1.6302.2.1.2.2.intf_id" />
        <value name="battery_current" source="1.3.6.1.4.1.6302.2.1.2.5.2.intf_id" />
        <value name="battery_voltage" source="1.3.6.1.4.1.6302.2.1.2.5.1.intf_id" />
    </data>

    <conversions>
        <function definition='${} 1000 /'>
            <data name="current" />
            <data name="voltage" />
            <data name="battery_current" />
            <data name="battery_voltage" />
        </function>
    </conversions>
</composite>
