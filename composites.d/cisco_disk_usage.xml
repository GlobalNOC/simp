<?xml version="1.0" encoding="UTF-8" ?>

<composite>
    <variables>
        <scan value="partition_name" oid="1.3.6.1.4.1.9.9.10.1.1.4.1.1.10.**">
            <match name="partition_idx"/>
        </scan>
    </variables>

    <data>
        <meta name="name" source="partition_name" />
        <value name="disk_total" source="1.3.6.1.4.1.9.9.10.1.1.4.1.1.4.partition_idx" />
        <value name="disk_free" source="1.3.6.1.4.1.9.9.10.1.1.4.1.1.5.partition_idx" />
        <value name="disk_used" source="1.3.6.1.4.1.9.9.10.1.1.4.1.1.5.partition_idx" />
    </data>

    <conversions>
        <function definition='${disk_total} ${disk_free} -'>
            <data name="disk_used" />
        </function>

        <drop>
            <data name="disk_free" />
        </drop>
    </conversions>
</composite>
