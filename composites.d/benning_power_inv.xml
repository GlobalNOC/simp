<?xml version="1.0" encoding="UTF-8" ?>

<composite>
    <variables>
        <scan value="inlet_name" oid="1.3.6.1.4.1.1356.1.7.2.2.1.5.*">
            <match name="inlet_id"/>
        </scan>
    </variables>

    <data>
        <meta name="intf" source="intf_id" />
        <value name="voltage" source="1.3.6.1.4.1.1356.1.7.2.2.1.5.intf_id" />
        <value name="current" source="1.3.6.1.4.1.1356.1.7.2.2.1.6.intf_id" />
    </data>

    <conversions>
        <function definition='0 == 0 ${} ifelse'>
            <data name="voltage" />
            <data name="current" />
        </function>

        <replace this='${}' with='Inverter ${} Output'>
            <data name="intf" />
        </replace>
    </conversions>
</composite>
