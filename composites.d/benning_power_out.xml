<?xml version="1.0" encoding="UTF-8" ?>

<composite>
    <variables>
        <constant name="intf_id" value="1" />
    </variables>

    <data>
        <meta name="intf" source="intf_id" />
        <value name="voltage" source="1.3.6.1.4.1.1356.1.7.1.2.1.7.1" />
        <value name="current" source="1.3.6.1.4.1.1356.1.7.1.2.1.8.1" />
    </data>

    <conversions>
        <replace this='${}' with='Static Bypass Output ${}'>
            <data name="intf" />
        </replace>

        <function definition='0 == 0 ${} ifelse'>
            <data name="voltage" />
            <data name="current" />
        </function>
    </conversions>
</composite>
